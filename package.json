    "css-loader": "^0.28.10",
    "enzyme": "^2.4.1",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.4.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-react": "^6.2.0",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "graphql": "0.13.0",
    "graphql-tag": "^2.8.0",
