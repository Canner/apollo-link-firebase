    "karma-mocha": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "mocha": "^2.4.5",
    "precommit-hook-eslint": "^3.0.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.3.1",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.20.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
